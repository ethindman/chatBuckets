<h1>Latest Question Lists</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @question_lists.each do |question_list| %>
      <tr>

        <td><%= link_to question_list.title, question_list %></td>
        <td><%= question_list.description %></td>
      </tr>
    <% end %>
  </tbody>
</table>
